<div class="nested-fields">
  <div class="row">
    <div class="input-field">
      <%= text_field_tag :nome, f.object.user.present? ? f.object.user.email : nil, class: 'js-name-collaborator autocomplete', placeholder: 'Colaborador', autocomplete: "off" %>
      <%= f.input :user_id, label: false, input_html: { class: "js-id-collaborator hidden" } %>
    </div>
    <%= link_to_remove_association material_icon.close.md_18, f, class: 'js-remove grey-text text-darken-2 hidden' %>
  </div>
</div>